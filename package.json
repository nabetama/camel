{
  "name": "ts-camelize",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "codecov": "yarn test --coverage && codecov",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "tests/.*test.(ts|js)"
  },
  "repository": "git+ssh://git@github.com/nabetama/ts-camelize.git",
  "keywords": ["transform", "camel-case"],
  "author": "Mao Nabeta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabetama/ts-camelize/issues"
  },
  "homepage": "https://github.com/nabetama/ts-camelize#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
    "@babel/preset-env": "^7.15.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "codecov": "^3.7.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "npm": "^7.21.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "yarn": "^1.22.11"
  }
}
