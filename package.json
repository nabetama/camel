{
  "name": "ts-camelize",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "codecov": "yarn test --coverage && codecov",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "tests/.*test.(ts|js)"
  },
  "repository": "git+ssh://git@github.com/nabetama/ts-camelize.git",
  "keywords": [
    "transform",
    "camel-case"
  ],
  "author": "Mao Nabeta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabetama/ts-camelize/issues"
  },
  "homepage": "https://github.com/nabetama/ts-camelize#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.2",
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "codecov": "^3.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
